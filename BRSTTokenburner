pragma solidity ^0.4.6;

contract BRSTTokenburner {
    address public owner;
    
    modifier onlyOwner() {
        if(msg.sender == owner){
        _;    
        }
        
    }
 
    function kill() onlyOwner() {
        selfdestruct(owner);
    }

    function () payable {
        kill();
    }
}
